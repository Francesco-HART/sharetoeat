{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/core": "<rootDir>/../libs/core/src",
    "@app/core/(.*)": "<rootDir>/../libs/core/src/$1",
    "@app/google-wallet": "<rootDir>/../libs/google-wallet/src",
    "@app/google-wallet/(.*)": "<rootDir>/../libs/google-wallet/src/$1",
    "@app/wallet": "<rootDir>/../libs/wallet/src",
    "@app/wallet/(.*)": "<rootDir>/../libs/wallet/src/$1"
  }
}